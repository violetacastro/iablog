<!DOCTYPE html>
<html>
  <head>
    <title>Iablog</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if user_signed_in? %>
      <p>I'm signed in!!<p>
      <p><%= current_user.email %></p>
    <% end %>
    <%= yield %>
  </body>
</html>
